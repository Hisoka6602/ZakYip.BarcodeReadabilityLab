{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/staging/barcode-lab-.log",
          "rollingInterval": "Hour",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 52428800,
          "retainedFileCountLimit": 168,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] [{Level:u3}] [{SourceContext}] {Message:lj} {Properties:j}{NewLine}{Exception}",
          "shared": false,
          "flushToDiskInterval": "00:00:01"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "BarcodeReadabilityService": {
    "MonitorPath": "/var/barcode-lab/staging/Monitor",
    "UnableToAnalyzePath": "/var/barcode-lab/staging/UnableToAnalyze",
    "TrainingDataPath": "/var/barcode-lab/staging/TrainingData",
    "ModelPath": "/var/barcode-lab/staging/Models"
  },
  "BarcodeMlModel": {
    "CurrentModelPath": "/var/barcode-lab/staging/Models/noread-classifier-current.zip"
  },
  "BarcodeAnalyzerOptions": {
    "WatchDirectory": "/var/barcode-lab/staging/Monitor",
    "UnresolvedDirectory": "/var/barcode-lab/staging/UnableToAnalyze"
  },
  "TrainingOptions": {
    "TrainingRootDirectory": "/var/barcode-lab/staging/TrainingData",
    "OutputModelDirectory": "/var/barcode-lab/staging/Models"
  }
}
